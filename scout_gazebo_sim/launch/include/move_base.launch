<?xml version="1.0"?>



<launch>
  <arg name="planner"/>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find scout_control)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find scout_control)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />

    <rosparam file="$(find scout_control)/params/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find scout_control)/params/global_costmap_params.yaml" command="load" />

    <rosparam file="$(find scout_control)/params/$(arg planner)_local_planner_params.yaml" command="load" />
    
    <rosparam file="$(find scout_control)/params/move_base_params.yaml" command="load" />
    <!-- <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS"/> -->
  </node>
  
</launch> 
